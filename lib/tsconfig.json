{
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./dist",
    "strictNullChecks": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "include": [
    "constants/**/*.ts",
    "dtos/**/*.ts",
    "msgs/**/*.ts",
    "types/**/*.ts",
    "interceptors/**/*.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
